# GenieNest Development Environment Variables

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT="blog-sample-381923"
GOOGLE_CLOUD_LOCATION="us-central1"

# ADK Configuration
GOOGLE_GENAI_USE_VERTEXAI="True"

# Gemini API Configuration (Alternative to Vertex AI)
# GEMINI_API_KEY="your-gemini-api-key"

# FastAPI Configuration
ENV="development"
PORT=8080
CORS_ORIGINS="http://localhost:3000,http://localhost:3001"

# Logging
LOG_LEVEL="DEBUG"

# Session Management (Future use)
# REDIS_URL="redis://localhost:6379"

# Security (Future use)
# SECRET_KEY="your-secret-key"
# JWT_SECRET="your-jwt-secret"

# PostgreSQL Configuration
DATABASE_TYPE=postgresql
DATABASE_URL=postgresql://genieus_user:genieus_password@localhost:5432/genieus_db
POSTGRES_USER=genieus_user
POSTGRES_PASSWORD=genieus_password
POSTGRES_DB=genieus_db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432