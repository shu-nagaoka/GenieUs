# ========== GenieUs Local Development Environment ==========
# このファイルはローカル開発用の環境変数設定です
# OAuth認証のテスト等で使用します

# GCP設定
GCP_PROJECT_ID=blog-sample-381923
GCP_REGION=asia-northeast1
ENVIRONMENT=local

# サービス名（ローカルでは使用しない）
BACKEND_SERVICE_NAME=genius-backend-local
FRONTEND_SERVICE_NAME=genius-frontend-local

# Backend設定
BACKEND_PORT=8080
GOOGLE_GENAI_USE_VERTEXAI=True
GOOGLE_CLOUD_LOCATION=us-central1
ROUTING_STRATEGY=enhanced
LOG_LEVEL=DEBUG
DATABASE_URL=sqlite:///./data/genieus.db
CORS_ORIGINS=http://localhost:3000,http://localhost:30001

# Frontend設定
FRONTEND_PORT=3000
NODE_ENV=development
NEXT_PUBLIC_API_BASE_URL=http://localhost:8080
NEXTAUTH_URL=http://localhost:3000

# OAuth設定（ローカル開発用）
# Google Cloud ConsoleでリダイレクトURLを以下に設定:
# http://localhost:3000/api/auth/callback/google
NEXTAUTH_SECRET=local-development-secret-change-in-production
GOOGLE_CLIENT_ID=your-local-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=GOCSPX-your-local-client-secret

# API Keys（必要に応じて）
GOOGLE_API_KEY=your-gemini-api-key
# GOOGLE_AIPSK=your-adk-api-key

# その他の開発設定
JWT_SECRET=dev-jwt-secret-key
JWT_EXPIRE_MINUTES=1440