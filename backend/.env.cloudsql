# ========== GenieUs Cloud SQL Production Environment ==========

# ========== Core Application ==========
APP_NAME=GenieUs
ENVIRONMENT=production
PORT=8080

# ========== Google Cloud Configuration ==========
GOOGLE_CLOUD_PROJECT=blog-sample-381923
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_GENAI_USE_VERTEXAI=True

# ========== Network Settings ==========
CORS_ORIGINS=https://genieus-frontend-staging-123456789-uc.a.run.app,https://genieus-frontend-123456789-uc.a.run.app

# ========== Database Configuration ==========
DATABASE_TYPE=postgresql

# ========== Cloud SQL PostgreSQL Configuration ==========
# Cloud SQL接続設定
CLOUD_SQL_CONNECTION_NAME=blog-sample-381923:us-central1:genieus-postgres
POSTGRES_USER=genieus_user
POSTGRES_PASSWORD=${POSTGRES_PASSWORD}
POSTGRES_DB=genieus_db

# ========== Security Settings (Production) ==========
JWT_SECRET=${JWT_SECRET}
JWT_EXPIRE_MINUTES=1440

# ========== NextAuth.js Configuration ==========
NEXTAUTH_SECRET=${NEXTAUTH_SECRET}
NEXTAUTH_URL=https://genieus-frontend-123456789-uc.a.run.app

# ========== Google OAuth Configuration ==========
GOOGLE_CLIENT_ID=${GOOGLE_CLIENT_ID}
GOOGLE_CLIENT_SECRET=${GOOGLE_CLIENT_SECRET}

# ========== File Storage ==========
BUCKET_NAME=genieus-files-prod

# ========== Logging ==========
LOG_LEVEL=INFO
LOG_FORMAT=json

# ========== Routing ==========
ROUTING_LOG_LEVEL=INFO