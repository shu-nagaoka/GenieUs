# ========== GenieUs Production Environment ==========

# ========== Core Application ==========
APP_NAME=GenieUs
ENVIRONMENT=production
PORT=8080

# ========== Google Cloud Configuration ==========
GOOGLE_CLOUD_PROJECT=blog-sample-381923
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_GENAI_USE_VERTEXAI=True
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# ========== Network Settings ==========
CORS_ORIGINS=https://genieus-frontend.run.app
ALLOWED_ORIGINS=https://genieus-frontend.run.app

# ========== Database Configuration ==========
# PostgreSQL for production
DATABASE_URL=postgresql://user:pass@host:port/genieus_prod
DATABASE_TYPE=postgresql

# ========== Security Settings (Production) ==========
JWT_SECRET=your_production_jwt_secret_key
JWT_EXPIRE_MINUTES=1440

# ========== File Storage ==========
BUCKET_NAME=genieus-files-prod

# ========== Logging ==========
LOG_LEVEL=INFO
LOG_FORMAT=json

# ========== Routing Strategy ==========
ROUTING_STRATEGY=keyword
HYBRID_KEYWORD_WEIGHT=0.4
HYBRID_LLM_WEIGHT=0.6
ENABLE_AB_TEST=false
AB_TEST_RATIO=0.5
ROUTING_LOG_LEVEL=INFO
COLLECT_ROUTING_METRICS=true

# ========== Advanced Routing (Future) ==========
# METRICS_EXPORT_INTERVAL=3600
# LLM_ROUTING_TEMPERATURE=0.1
# LLM_ROUTING_MAX_TOKENS=200
# LLM_ROUTING_TIMEOUT=5000
# ENABLE_ROUTING_FALLBACK=true
# FALLBACK_STRATEGY=keyword

# ========== NextAuth.js (Production) ==========
GOOGLE_CLIENT_ID=your_google_oauth_client_id_prod
GOOGLE_CLIENT_SECRET=your_google_oauth_client_secret_prod
NEXTAUTH_SECRET=your_nextauth_secret_key_prod
NEXTAUTH_URL=https://genieus-frontend.run.app

# ========== Optional API Keys ==========
GOOGLE_API_KEY=your-gemini-api-key
GOOGLE_AIPSK=your-adk-api-key

# ========== Cloud Run Settings ==========
PYTHONPATH=/app