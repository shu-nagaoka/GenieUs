# ========== GenieUs PostgreSQL Environment ==========

# ========== Core Application ==========
APP_NAME=GenieUs
ENVIRONMENT=development
PORT=8000

# ========== Google Cloud Configuration ==========
GOOGLE_CLOUD_PROJECT=blog-sample-381923
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_GENAI_USE_VERTEXAI=True

# ========== Network Settings ==========
CORS_ORIGINS=http://localhost:3000,http://localhost:30001

# ========== Database Configuration ==========
DATABASE_TYPE=postgresql

# ========== PostgreSQL Configuration ==========
# ローカル開発用PostgreSQL設定
POSTGRES_USER=genieus_user
POSTGRES_PASSWORD=genieus_password
POSTGRES_DB=genieus_db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Cloud SQL設定（本番・ステージング用）
# CLOUD_SQL_CONNECTION_NAME=blog-sample-381923:us-central1:genieus-postgres
# POSTGRES_USER=genieus_user
# POSTGRES_PASSWORD=your_secure_password
# POSTGRES_DB=genieus_db

# ========== Security Settings (Development) ==========
JWT_SECRET=your-development-jwt-secret-key-change-in-production
JWT_EXPIRE_MINUTES=1440

# ========== NextAuth.js Configuration ==========
NEXTAUTH_SECRET=your-development-nextauth-secret-change-in-production
NEXTAUTH_URL=http://localhost:3000

# ========== Google OAuth Configuration ==========
# 開発環境では空文字列でOK（オプション認証）
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# ========== File Storage ==========
BUCKET_NAME=genieus-files

# ========== Logging ==========
LOG_LEVEL=INFO
LOG_FORMAT=json

# ========== Routing ==========
ROUTING_LOG_LEVEL=INFO