# ========== GenieUs Production Environment ==========

# ========== Core Application ==========
APP_NAME=GenieUs
ENVIRONMENT=production
PORT=8000

# ========== Google Cloud Configuration ==========
GOOGLE_CLOUD_PROJECT=your-project-id  # 要変更: 実際のプロジェクトID
GOOGLE_CLOUD_LOCATION=asia-northeast1
GOOGLE_GENAI_USE_VERTEXAI=True

# ========== Network Settings ==========
CORS_ORIGINS=https://genius-frontend-production.run.app

# ========== Database Configuration ==========
DATABASE_URL=sqlite:///./data/genieus.db
DATABASE_TYPE=sqlite

# ========== Security Settings (Production) ==========
JWT_SECRET=your-production-jwt-secret-key  # 要変更: 本番用シークレット
JWT_EXPIRE_MINUTES=1440

# ========== File Storage ==========
BUCKET_NAME=genieus-files-prod

# ========== Logging ==========
LOG_LEVEL=INFO
LOG_FORMAT=json

# ========== Routing Strategy ==========
ROUTING_STRATEGY=enhanced
HYBRID_KEYWORD_WEIGHT=0.4
HYBRID_LLM_WEIGHT=0.6
ENABLE_AB_TEST=false
AB_TEST_RATIO=0.5
ROUTING_LOG_LEVEL=INFO
COLLECT_ROUTING_METRICS=true

# ========== NextAuth.js (Production) ==========
NEXTAUTH_URL=https://genius-frontend-production.run.app  # 要変更: 実際のURL
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com  # 要変更
GOOGLE_CLIENT_SECRET=GOCSPX-your-secret  # 要変更
NEXTAUTH_SECRET=your-nextauth-secret  # 要変更

# ========== Required API Keys ==========
GOOGLE_API_KEY=your-gemini-api-key  # 要変更: Gemini API用
GOOGLE_AIPSK=your-adk-api-key  # 要変更: ADK API用